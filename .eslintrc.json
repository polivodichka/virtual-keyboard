{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "no-param-reassign": ["error", { "props": false }],
        "no-use-before-define": ["error", { "functions": false, "classes": true, "variables": true }]
    }
}
